<%- include('../include/navbar.ejs') %>
<div class="container">
<% if(employee) { %>
   <ul class="list-group">
   <% for(let obj of employee) { %>
      <li class="list-group-item d-flex justify-content-between align-items-center">
            <a href='/user/<%= obj.id %>' style="text-decoration: none; color: rgb(51, 48, 48);"><%= obj.userId %></a>
      <span class="badge badge-primary badge-pill"><%= obj.id %></span>
      </li>
   <% } %> 
   </ul>
<% }%>
</div>

