
  <% if(events) { %> 
   <div class="list-group">
   <% for(let obj of events) { %>
     <li class="list-group-item d-flex justify-content-between align-items-center">
         <a href="/news&event/<%= obj.id %>" style="text-decoration: none;"><%= obj.title %></a> 
         <span class="badge badge-primary badge-pill" onclick="newseventsDel('<%= obj.id %>')"> delete </span>
       </li>
   <% } %>
 </div>
 <% } else { %>
   <h1>You have no events.</h1>
 <% } %>
   
